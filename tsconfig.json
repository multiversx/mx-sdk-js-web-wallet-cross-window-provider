{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "outDir": "./out",
        "lib": ["dom", "es2015"],
        "module": "es2015",
        "moduleResolution": "Node",
        "target": "es2015",
        "skipLibCheck": true,
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "declarationDir": "./disoutt/types"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "out"
    ]
}
